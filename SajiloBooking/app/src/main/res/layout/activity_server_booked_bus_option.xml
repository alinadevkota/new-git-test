<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_server_booked_bus_option"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="nass.sajilobooking.server_booked_bus_option">
    <TextView
        android:id="@+id/server_booked_bus_options_tv1"
        android:text="Booked By"
        android:layout_gravity="center"
        android:textSize="37sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:layout_marginTop="17dp"
        android:id="@+id/server_booked_bus_options_ll1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView

            android:id="@+id/server_booked_bus_options_tv2"
            android:text="Name : "
            android:textSize="27sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_marginLeft="47dp"
            android:id="@+id/server_booked_bus_options_tv3"
            android:text=""
            android:textSize="27sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="17dp"
        android:id="@+id/server_booked_bus_options_ll2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView

            android:id="@+id/server_booked_bus_options_tv4"
            android:text="Address : "
            android:textSize="27sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_marginLeft="27dp"
            android:id="@+id/server_booked_bus_options_tv5"
            android:text=""
            android:textSize="27sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="17dp"
        android:id="@+id/server_booked_bus_options_ll3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView

            android:id="@+id/server_booked_bus_options_tv6"
            android:text="Email ID : "
            android:textSize="27sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_marginLeft="17dp"
            android:id="@+id/server_booked_bus_options_tv7"
            android:text=""
            android:textSize="27sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="17dp"
        android:id="@+id/server_booked_bus_options_ll4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView

            android:id="@+id/server_booked_bus_options_tv8"
            android:text="Phone Number:"
            android:textSize="27sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_marginLeft="7dp"
            android:id="@+id/server_booked_bus_options_tv9"
            android:text=""
            android:textSize="27sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</LinearLayout>
